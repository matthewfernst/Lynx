{
    "compilerOptions": {
        "noEmit": false,
        "target": "ESNext",
        "useDefineForClassFields": true,
        "lib": ["DOM", "ESNext"],
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "moduleResolution": "Node16",
        "isolatedModules": true
    },
    "$schema": "https://json.schemastore.org/tsconfig"
}
